{
  "manifest_version": 3,

  "name": "Google Finance Plus",
  "description": "",
  "version": "0.0.999",

  "options_ui": {
    "page": "options.html"
  },

  "action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "css": ["styles.css"],
          "js": ["js/vendor.js", "js/content_script.js"]
      }
  ],

  "permissions": [
    "storage"
  ]
}
